{
    "name": "Eye Lid",
    "description": "Only see what you want to see",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"],
    "background": {
        "scripts": [
            "tabUrlState.js",
            "background.js"]
    },
    "browser_action": {
        "default_title": "Eye Lid"
    }
}